  <!-- 按标签分类获取文章,数量暂定为15条 -->
	<select id="selectArticlesByTagId" parameterType="int"
	 	resultType="com.ublblog.model.Article">
		SELECT
			tb_article.ID,
			tb_article.title,
			tb_article.create_time,
			tb_article.read_count
		FROM
			tb_article,rel_article_tag
		WHERE 			
		 	tb_article.deleted=0
		 AND
			rel_article_tag.article_id=tb_article.id
		 AND 
		 	rel_article_tag.tag_id=#{tagId}

	</select>
